{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "jsx": "react-jsx",
    "baseUrl": "./src",
    "paths": {
      "@assets/*": [
        "./assets/*"
      ],
      "@components/*": [
        "./components/*"
      ],
      "@contexts/*": [
        "./contexts/*"
      ],
      "@pages/*": [
        "./pages/*"
      ],
      "@routes/*": [
        "./routes/*"
      ],
      "@services/*": [
        "./services/*"
      ],
      "@styles/*": [
        "./styles/*"
      ],
      "@utils/*": [
        "./utils/*"
      ],
      "@src/*": [
        "./*"
      ],
    }
  }
}
